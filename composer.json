{
    "name": "lordrhodos/github-release-to-changelog-converter",
    "description": "Converts the releases of a Github repository into a changelog based on https://keepachangelog.com/en/1.0.0/",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Rodacker",
            "email": "patrick@rodacker.de"
        }
    ],
    "require": {
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lordrhodos\\GithubTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lordrhodos\\GithubTools\\Tests\\": "tests/"
        }
    }
}
