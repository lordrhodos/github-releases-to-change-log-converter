{
    "name": "lordrhodos/github-release-to-changelog-converter",
    "description": "Converts the releases of a Github repository into a changelog",
    "type": "project",
    "license": "MIT",
    "version": "0.2.0",
    "authors": [
        {
            "name": "Patrick Rodacker",
            "email": "patrick@rodacker.de"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symfony/console": "^4.2",
        "php-http/guzzle6-adapter": "^1.1",
        "knplabs/github-api": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "php-http/mock-client": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Lordrhodos\\GithubTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lordrhodos\\GithubTools\\Tests\\": "tests/"
        }
    }
}
